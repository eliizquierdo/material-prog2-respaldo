         version="3.0">

    <servlet>
        <servlet-name>EjercicioServlet</servlet-name>
        <servlet-class>controlador.EjercicioServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EjercicioServlet</servlet-name>
